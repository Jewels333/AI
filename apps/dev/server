ASC_2 = 'apps/dev/terminal/ASC-2.csv'
def findSet(set, str):
    count = int(0)
    for element in set:
        count = int(count + 1)
        if element == str:
            return count
            continue
        elif element != str:
            continue
def retrieveSet(num, set):
    count1 = int(0)
    for element in set:
        count1 = int(count1 + 1)
        if count1 == num:
            return element
        else:
            continue
class maintenance:
    def daily():
        count2 = int(0)
        for element in codeKey2:
            count = int(count2 + 1)
            encoded = ''
            decoded = retrieveSet(count2, charList)
            print(decoded)
maintenance.daily()
