import socket
import time
s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
target = input('Enter Target:')
targetNum1 = (input('Enter port height:'))
if targetNum1 == 'inf':
    
    targetNum1 = int()
else:
    targetNum1 = int(targetNum1)
targetNum2 = int(input('Enter port start:'))
target_ip = socket.gethostbyname(target)
print('Starting scan on host:', target_ip)
def port_scan(port):
    try:
        s.connect((target_ip, port))
        return True
    except:
        return False
type1 = input('Which type?(1(show)/2(hide)/3(common))')
print('Starting')
if type1 == '1':

    start = time.time()
    for port in range(targetNum2, targetNum1):
        if port_scan(port):
            print(f'port {port} is open      !!!!')
        else:
            print(f'port {port} is closed')
if type1 == '2':
    start = time.time()
    for port in range(targetNum2, targetNum1):
        if port_scan(port):
            print(f'port {port} is open')
        else:
            time.time()
if type1 == '3':
    start = time.time()
    result = ''
    #Scan 1: 20
    if port_scan(20):
        result = result + '\n Port 20 is open'
    else:
        result = result + '\n Port 20 is closed'
    #Scan 2: 21
    if port_scan(21):
        result = result + '\n Port 21 is open'
    else:
        result = result + '\n Port 21 is closed'
    #Scan 3: 22
    if port_scan(22):
        result = result + '\n Port 22 is open'
    else:
        result = result + '\n Port 22 is closed'
    #Scan 4: 23
    if port_scan(23):
        result = result + '\n Port 23 is open'
    else:
        result = result + '\n Port 23 is closed'
    #Scan 5: 25
    if port_scan(25):
        result = result + '\n Port 25 is open'
    else:
        result = result + '\n Port 25 is closed'
    #Scan 6: 53
    if port_scan(53):
        result = result + '\n Port 53 is open'
    else:
        result = result + '\n Port 53 is closed'
    #Scan 7: 80
    if port_scan(80):
        result = result + '\n Port 80 is open'
    else:
        result = result + '\n Port 80 is closed'
    #Scan 8: 110
    if port_scan(110):
        result = result + '\n Port 110 is open'
    else:
        result = result + '\n Port 110 is closed'
    #Scan 9: 119
    if port_scan(119):
        result = result + '\n Port 119 is open'
    else:
        result = result + '\n Port 119 is closed'
    #Scan 10: 123
    if port_scan(123):
        result = result + '\n Port 123 is open'
    else:
        result = result + '\n Port 123 is closed'
    #Scan 11: 143
    if port_scan(143):
        result = result + '\n Port 143 is open'
    else:
        result = result + '\n Port 143 is closed'
    #Scan 12: 161
    if port_scan(161):
        result = result + '\n Port 161 is open'
    else:
        result = result + '\n Port 161 is closed'
    #Scan 13: 194
    if port_scan(194):
        result = result + '\n Port 194 is open'
    else:
        result = result + '\n Port 194 is closed'
    #Scan 14: 443
    if port_scan(443):
        result = result + '\n Port 443 is open'
    else:
        result = result + '\n Port 443 is closed'
    print(result)
    
end = time.time()
print(f'Time taken {end-start:.2f} seconds')
  